{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "plugins": ["prettier-plugin-jsdoc"],
  "jsdocSpaces": 1,
  "jsdocDescriptionWithDot": false,
  "jsdocKeepUnParseAbleExampleIndent": true,
  "jsdocPreferCodeFences": true
}
